<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/new">New Message</a></li>
      </ul>
    </nav>
    <div class="messages">
      <% messages.forEach(function(message) { %>
      <div class="message">
        <h2><%= message.user %></h2>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
      </div>
      <% }) %>
    </div>
  </body>
</html>
